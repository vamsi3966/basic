pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'hello world application buliding'
            }
        }
        stage('test') {
            steps {
                echo ' hello world application testing'
            }
            
        }
        stage('deploy') {
            steps {
                echo ' hello world application deploying'
            }
            
        }
        stage('release') {
            steps {
                echo 'hello world application realesing'
            }
            
        }
    }
}
